    "express": "^4.17.1",
    "facebook-chat-api": "github:Schmavery/facebook-chat-api",
    "html-entities": "^1.3.1",
    "http": "0.0.0",
    "humanize-duration": "^3.23.1",
    "jimp": "^0.9.8",
    "memjs": "^1.2.2",
    "messenger-botcore": "github:AstroCB/BotCore",
    "pm2": "^4.4.1",
