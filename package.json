{
  "name": "zero-bot",
  "version": "1.0.0",
  "description": "A group chat bot for Otakuverse that performs a variety of functions.",
  "main": "src/main.js",
  "start": "node src/main.js",
 "engines": {
  "node": "18.x"
}

  },
  "scripts": {
    "start": "pm2-runtime start src/main.js --name 'zero-bot'"
